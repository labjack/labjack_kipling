{
	"logging_config": {
		"file_prefix": "basic_config",
		"write_to_file": false,
		"default_result_view": "0",
		"default_result_file": "0"
	},


	"view_config": {
		"update_rate_ms": 100
	},
	"views": [
		"basic_view",
		"current_values_view"
	],
	"basic_view": {
		"name": "Basic Graph",
		"view_type": "basic_graph",
		"window_size": 100,
		"group": "basic_data_group"
	},
	"current_values_view": {
		"name": "Current Values",
		"view_type": "current_values",
		"group": "basic_data_group"
	},


	"data_groups": [
		"basic_data_group"
	],
	"basic_data_group": {
		"group_name": "basic_data_group",
		"group_period_ms": 100,
		"is_stream": false,

		"device_serial_numbers": ["1", "2"],
		"1": {
			"registers": [{
				"name": "AIN0",
				"format": "default"
			}]
		},
		"2": {
			"registers": [{
				"name": "AIN0",
				"format": "custom_func",
				"format_func": "console.log('Custom Func', val); return val;"
			}]
		},
		
		"logging_options": {
			"write_to_file": false,
			"file_prefix": "basic_group",
			"max_samples_per_file": 100,
			"data_collector_config": {
				"REPORT_DEVICE_IS_ACTIVE_VALUES": true,
				"REPORT_DEFAULT_VALUES_WHEN_LATE": false
			}
		}
	}
}