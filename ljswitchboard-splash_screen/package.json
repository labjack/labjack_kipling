{
  "name": "ljswitchboard-splash_screen",
  "version": "0.0.4",
  "private": true,
  "description": "The splash screen & primary loading file for LabJack's Kipling program.",
  "author": {
    "name": "Chris Johnson",
    "url": "https://labjack.com"
  },
  "homepage": "https://github.com/labjack/labjack_kipling/tree/master/ljswitchboard-splash_screen",
  "repository": {
    "type": "git",
    "url": "https://github.com/labjack/labjack_kipling.git",
    "directory": "ljswitchboard-splash_screen"
  },
  "keywords": [
    "LabJack",
    "LJM",
    "T7",
    "ljswitchboard"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/labjack/labjack_kipling/issues"
  },
  "main": "lib/index.html",
  "node-main": "lib/main.js",
  "window": {
    "title": "Kipling",
    "icon": "lib/Kipling_128x128.png",
    "toolbar": true,
    "frame": true,
    "width": 480,
    "height": 300,
    "min_width": 480,
    "min_height": 300,
    "show": true,
    "resizable": true
  },
  "nodejs": true,
  "dependencies": {
    "async": "3.2.2",
    "co": "^4.6.0",
    "concat-stream": "1.5.2",
    "decache": "^4.5.0",
    "fs-extra": "0.18.4",
    "fs.extra": "1.3.2",
    "ljswitchboard-ljm_driver_checker": "*",
    "ljswitchboard-package_loader": "*",
    "ljswitchboard-require": "*",
    "ljswitchboard-window_manager": "*",
    "q": "1.5.1",
    "rimraf": "2.2.8",
    "walk": "^2.3.14",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "nw": "0.12.3"
  },
  "un-managed-dependencies": {
    "semver": "4.2.0",
    "unzipper": "0.10.11",
    "nw": "^0.31.5",
    "nodewebkit": "0.11.6",
    "ljswitchboard-core": "*"
  },
  "script_notes": "to run a script w/o using npm test, use npm run [script name]",
  "scripts": {
    "start": "npm run nw",
    "nodewebkit": "nodewebkit",
    "nw": "nw"
  },
  "enableStartupConfig": false,
  "buildType": "release",
  "test": false,
  "clearCachesOnModuleLoad": true,
  "testPersistentDataFolderName": "K3_DEV",
  "performLiveTests": false,
  "webkit": {
    "plugin": true
  },
  "js-flags": "--expose-gc",
  "persistentDataFolderName": "K3",
  "persistentDataVersion": "0.0.1",
  "forceRefreshOfPersistentData": false,
  "enableBetaOptions": true
}
