{
	"name": "ljswitchboard-builder",
	"description": "Building and packing for the ljswitchboard program.",
	"version": "3.0.7",
	"private": true,
	"homepage": "https://github.com/labjack/labjack_kipling/tree/master/ljswitchboard-builder",
	"repository": {
		"type": "git",
		"url": "https://github.com/labjack/labjack_kipling.git",
		"directory": "ljswitchboard-builder"
	},
	"license": "MIT",
	"dependencies": {
		"adm-zip": "0.4.13",
		"archiver": "3.1.1",
		"async": "3.1.0",
		"colors": "1.4.0",
		"concat-stream": "1.5.2",
		"decompress": "4.2.1",
		"decompress-unzip": "3.2.2",
		"decompress-zip": "0.2.2",
		"extract-zip": "1.7.0",
		"fs-extra": "^9.0.1",
		"fs.extra": "^1.3.2",
		"handlebars": "4.7.6",
		"jszip": "2.5.0",
		"node-zip": "1.1.1",
		"q": "1.5.1",
		"rimraf": "2.2.8",
		"targz": "1.0.1",
		"underscore": "1.9.1",
		"unzipper": "0.10.11",
		"yauzl": "2.10.0",
		"yazl": "2.5.1"
	},
	"un-managed-dependencies": {
		"semver": "4.2.0",
		"unzipper": "0.10.11"
	},
	"kipling_dependencies": [
		"ljswitchboard-electron_main",
		"ljswitchboard-electron_splash_screen",
		"ljswitchboard-io_manager",
		"ljswitchboard-kipling",
		"ljswitchboard-module_manager",
		"ljswitchboard-static_files",
		"ljswitchboard-kipling_tester"
	],
	"splash_screen_build_keys": {
		"window": {
			"title": "Kipling",
			"icon": "lib/Kipling_128x128.png",
			"toolbar": false,
			"frame": false,
			"width": 480,
			"height": 300,
			"min_width": 480,
			"min_height": 300,
			"show": true,
			"resizable": false
		},
		"enableStartupConfig": false,
		"buildType": "release",
		"test": false,
		"clearCachesOnModuleLoad": false,
		"enableBetaOptions": false
	},
	"kipling_test_dependencies": [
		"ljswitchboard-kipling_tester"
	],
	"kipling_primary_dependency": "ljswitchboard-electron_main",
	"devDependencies": {
		"chai": "^4.2.0",
		"diff": ">=3.5.0",
		"mocha": "7.2.0",
		"electron-builder": "^22.9.1"
	},
	"directories": {
		"buildResources": "branding_files"
	},
	"build": {
		"productName": "kipling",
		"appId": "com.labjack/kipling",
		"mac": {
			"icon": "Kipling3.icns",
			"category": "public.app-category.utilities"
		},
		"linux": {
			"icon": "Kipling_128x128.png"
		},
		"win": {
			"icon": "Kipling3.ico"
		},
		"asar": false,
		"npmArgs": ["--force"]
	},
	"script_notes": "to run a script w/o using npm test, use npm run [script name]",
	"scripts": {
		"clean": "rimraf temp_project_files temp* output",
		"build": "node ./build_scripts/build_project.js"
	}
}
