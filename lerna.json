{
  "npmClient": "npm",
  "packages": [
    "./*"
  ],
  "command": {
    "bootstrap": {
      "npmClientArgs": ["--no-package-lock", "-f", "--build-from-resource"]
    }
  },
  "version": "independent",
  "ignoreChanges": [
    "test*/**",
    "**/*.md",
    "deploy/**",
    "third_party_code/**",
    "temp*/**",
    "jshint*"
  ]
}
